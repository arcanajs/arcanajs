  "scripts": {
    "build": "tsc -p tsconfig.json --emitDeclarationOnly && webpack --config webpack.config.ts && cp -r src/types dist/ && node scripts/post-build.js && node scripts/create-entry-points.js",
    "update": "node scripts/update-packages.js",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "exports": {
    "./express-types": {
      "types": "./dist/types/express.d.ts"
    },
    "./server": {
      "types": "./dist/lib/index.server.d.ts",
      "default": "./dist/arcanajs.js"
    },
    "./arcanox": {
      "types": "./dist/lib/index.arcanox.d.ts",
      "default": "./dist/arcanox.js"
    },
    "./client": {
      "types": "./dist/lib/index.client.d.ts",
      "default": "./dist/arcanajs.client.js"
    },
