  },
  "version": "4.0.0",
  "description": "ArcanaJS Framework",
  "main": "./dist/arcanajs.js",
  "scripts": {
    "build": "tsc -p tsconfig.json --emitDeclarationOnly && webpack --config webpack.config.ts"
  },
  "exports": {
    "./arcanox": {
      "types": "./dist/lib/index.arcanox.d.ts",
      "default": "./dist/arcanox.js"
    },
    "./client": {
      "types": "./dist/lib/index.client.d.ts",
      "default": "./dist/arcanajs.client.js"
    },
    "./server": {
      "types": "./dist/lib/index.server.d.ts",
      "default": "./dist/arcanajs.js"
    }
  },
  "bin": {
    "arcanajs": "./bin/arcanajs.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "peerDependenciesMeta": {
    "pg": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "@faker-js/faker": {
      "optional": true
    },
    "mongodb": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.22",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "html-webpack-plugin": "^5.6.5",
    "mini-css-extract-plugin": "^2.9.4",
    "null-loader": "^4.0.1",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "reflect-metadata": "^0.2.2",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.17",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@types/pg": "^8.15.6",
    "@types/webpack-node-externals": "^3.0.4",
    "mongodb": "^7.0.0",
    "mysql2": "^3.15.3",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "keywords": [
    "arcanajs",
    "framework",
    "react",
    "express",
    "nodejs",
    "typescript",
    "javascript",
    "web",
    "server",
    "client",
    "routing",
    "router"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/arcanajs/arcanajs.git"
  },
  "bugs": {
